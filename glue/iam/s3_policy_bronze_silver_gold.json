{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "ListPrefixes",
			"Effect": "Allow",
			"Action": "s3:ListBucket",
			"Resource": "arn:aws:s3:::bus-insights-dev-us-east-1",
			"Condition": {
				"StringLike": {
					"s3:prefix": [
						"bronze/*",
						"silver/*",
						"athena/*"
					]
				}
			}
		},
		{
			"Sid": "ReadBronze",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject"
			],
			"Resource": "arn:aws:s3:::bus-insights-dev-us-east-1/bronze/*"
		},
		{
			"Sid": "WriteSilver",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::bus-insights-dev-us-east-1/silver/*"
		},
		{
			"Sid": "WriteGold",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::bus-insights-dev-us-east-1/gold/*"
		},
		{
			"Sid": "AthenaResultsRW",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::bus-insights-dev-us-east-1/athena/*"
		}
	]
}
