{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowPassRoleToGlue",
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": "arn:aws:iam::REDACTED:role/biz-insights-glue-athena-s3-role",
            "Condition": {
                "StringEquals": {
                    "iam:PassedToService": "glue.amazonaws.com"
                }
            }
        }
    ]
}
